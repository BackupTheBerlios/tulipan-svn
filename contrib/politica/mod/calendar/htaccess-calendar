#
# Calendar
#
RewriteRule ^([A-Za-z0-9]+)\/calendar(\/)?$ mod/calendar/index.php?calendar_name=$1
RewriteRule ^([A-Za-z0-9]+)\/calendar\/rss(\/)?$ mod/calendar/rss2.php?calendar_name=$1
RewriteRule ^([A-Za-z0-9]+)\/calendar\/([0-9]+)\.html$ mod/calendar/dayview.php?calendar_name=$1&event_id=$2

